close all;
clear all;
clc;

x = categorical({'Filament'; 'Model'; 'third'});
x = [1;2;3]
y = [0.415047; 0.41697; 0.58];
z = [0.11398; 0.013588; 0.18];
nil = zeros;
bar(x, [y nil], 'grouped')
ylabel('Mass reduction (%)');
yyaxis right
bar(x, [nil z], 'grouped')
ylabel('Weight (g)');

return;
z=[ 1 ; 3 ; 2.5 ; 5 ; 4 ; 2 ];
x=[0:size(z)]
Sigma = sum(exp(z));

for i=1:size(z)
    sigma(i) = exp(z(i))/Sigma
end;
    nil = x;
bar (x, [sigma z], 'grouped')
ylabel('Proba');
